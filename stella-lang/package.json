{
  "name": "stella-lang",
  "displayName": "Stella Lang",
  "description": "Syntax highlighting for the Stella programming language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "stella",
      "aliases": ["Stella", "stella"],
      "extensions": [".ste"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "stella",
      "scopeName": "source.stella",
      "path": "./syntaxes/stella.tmLanguage.json"
    }]
  }
}
