{
  "publisher": "StellaLang",
  "name": "stella-lang",
  "displayName": "Stella Lang",
  "description": "Syntax highlighting for the Stella programming language",
  "icon": "logo.png",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "stella",
        "aliases": [
          "Stella",
          "stella"
        ],
        "extensions": [
          ".ste"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "stella",
        "scopeName": "source.stella",
        "path": "./syntaxes/stella.tmLanguage.json"
      }
    ]
  }
}
